{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend of mobile application",
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc -p . --outDir dist",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm migration:run -- -d ./src/config/ormconfig.ts",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/migration",
    "migration:generate": "npm run typeorm -- -d ./src/config/ormconfig.ts migration:generate ./src/migrations/migration",
    "migration:revert": "npm run typeorm -- -d ./src/config/ormconfig.ts migration:revert"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.577.0",
    "@aws-sdk/credential-provider-env": "^3.577.0",
    "@azure/storage-blob": "^12.18.0",
    "@types/bcryptjs": "^2.4.6",
    "auth0": "^4.4.0",
    "aws-sdk": "^2.1623.0",
    "axios": "^1.6.8",
    "azure-storage": "^2.10.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-jwt": "^8.4.1",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-openid-connect": "^2.17.1",
    "glob": "^10.3.15",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.7.5",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/plugin-transform-object-rest-spread": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/types": "^7.24.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^9.0.8",
    "babel-plugin-add-module-exports": "^1.0.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
